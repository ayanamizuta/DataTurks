  "description": "API marketplace",
  "author": "DataTurks <admin@dataturks.com>",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikras/react-redux-universal-hot-example.git"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
